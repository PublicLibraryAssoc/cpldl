<footer class="footer-primary <%= color_class %>">
  <div class="container">
    <div class="row">
      <div class="threecol">
        <ul class="cms_nav">
          <strong>LEARN MORE</strong>
          <% if @footer_pages %>
            <% @footer_pages.each do |page| %>
              <li><%= link_to page.title, cms_page_path(page) %></li>
            <% end %>
          <% end %>
        </ul>
      </div>
      <%= render "shared/logo_footer" %>
    </div>
  </div>

<% if Rails.env == "stage" ||  Rails.env == "development"
  server_name = subdomain + "-stage" %>
  <% if request.subdomain == 'demo-stage' || request.subdomain == 'demo'
    server_name = request.subdomain %>
  <% end %>
  <%= Rails.env + " server: " + server_name   %>
<% end %>

</footer>