<footer class="footer-primary">
  <div class="container">
    <div class="row">
      <div class="threecol">
        <nav role="navigation">
          <ul class="cms_nav" role="menubar">
            <strong>LEARN MORE</strong>
            <% if @footer_pages %>
              <% @footer_pages.each do |page| %>
                <li role="presentation"><%= link_to page.title, cms_page_path(page), {"role" => "menuitem"} %></li>
              <% end %>
            <% end %>
          </ul>
       </nav> 
      </div>
      <%= render "shared/logo_footer" %>
      <div class="ninecol license">
      This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 Unported License
      </div>
    </div>
  </div>
</footer>
<%= render "shared/ga" if Rails.env.integration? %>
<%= render "shared/ga_event_tracking" if Rails.env.integration? %>
