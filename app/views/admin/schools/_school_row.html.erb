<li class="resource-row <%= school.enabled? ? "table_row_enabled" : "table_row_disabled" %>" id="school_row_<%= school.id %>" >
  <div class="resource-title">
    <%= school.school_name %>
  </div>
  <div class="resource-link">
    <%= link_to (school.enabled? ? "Disable" : "Enable"), admin_school_toggle_path(school.id), method: :post, remote: true %>
  </div>
</li>