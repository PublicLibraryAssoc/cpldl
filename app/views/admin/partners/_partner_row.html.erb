<li class="resource-row" id="partner_row_<%= partner.id %>">
  <div class="resource-cell">
    <%= partner.name %>
  </div>
  <div class="resource-cell">
    <%= link_to 'Delete', admin_partner_path(partner), method: :delete, remote: true, data: { confirm: "Delete #{partner.name} as a partner?" }, id: "delete_partner_link_#{partner.id}" %>
  </div>
</li>